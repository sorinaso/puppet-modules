production:
  delivery_method: :smtp
  smtp_settings:
    tls: true
    enable_starttls_auto: true
    address: "smtp.gmail.com"
    port: '587'
    domain: ""
    authentication: :plain
    user_name: "<%= @notifications_gmail_username %>"
    password: "<%= @notifications_gmail_password %>"

test:
  delivery_method: :test
